#/bin/bash

alias mkdir='mkdir -p'

#ls
alias la='ls -la'
alias ll='ls -l'

#cd ..
alias u='cd ../'
alias uu='cd ../../'
alias uuu='cd ../../../'
alias uuuu='cd ../../../../'

#apt-get
alias ud='sudo apt-get update && sudo apt-get upgrade'
alias ug='sudo apt-get dist-upgrade'
alias ar='sudo apt-get autoremove'

#hardware
alias backlight='xbacklight -set'

#ssh
alias sunlab='aaw218@sunlab.cse.lehigh.edu'
alias lehigh='aaw218@ssh.lehigh.edu'

